def prime_number(start,end):
    print("Prime Numbers: ")
    for c in range (start,end):
        for x in range(start,c):
            if c%x==0:
                break
        else:
             print(f"{c: 15}")


prime_number(2,20)


----------------------------------------

def primenumber(start,end):
    a=[]
    for x in range(start,end):
        for n in range(start,x):
            if x%n==0:
                break
        else:
            a.append(x)
    print(*a)


primenumber(2,30)
